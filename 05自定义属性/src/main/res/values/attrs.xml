<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--定义名字叫myAttributeView的属性集合-->
    <declare-styleable name="myAttributeView">
        <!--定义一个名字叫my_age，并且类型是integer的属性-->
        <attr name="my_age" format="integer"/>
        <!--定义一个名字叫my_name，并且类型是string的属性-->
        <attr name="my_name" format="string"/>
        <!--定义一个名字叫my_background，并且类型是reference|color的属性-->
        <attr name="my_background" format="reference|color"/>
    </declare-styleable>
</resources>